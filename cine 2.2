Funcion cancelarPedido(matriz Por Referencia,entradaFila, entradaColumna,pedidoEntradas,c_C1 Por Referencia, c_C2 Por Referencia , c_C3 Por Referencia, pBolsa Por Referencia, pBalde Por Referencia, pLata Por Referencia, coca Por Referencia, sprite Por Referencia, agua Por Referencia, mym  Por Referencia, mogul Por Referencia, beldent Por Referencia,contPedidos Por Referencia, pedidoCombo1,pedidoCombo2 ,pedidoCombo3 ,pedidoBolsa ,pedidoBalde ,pedidoLata,pedidoCoca ,pedidoSprite ,pedidoAgua ,pedidoMym,pedidoMogul,pedidoBeldent,contEntr Por Referencia,cantEntradas) 
	Borrar Pantalla
	Escribir "VOLVIENDO AL MENU PRINCIPAL"
	//"[ ]"
	si contPedidos>1 Entonces
		
		Para j=pedidoEntradas[contPedidos-1]+1 Hasta pedidoEntradas[contPedidos] Con Paso 1 Hacer
			matriz[entradaFila[j],entradaColumna[j]]="[ ]"
		Fin Para
		
		//Le asigno al contador el anterior
		c_C1=pedidoCombo1[contPedidos-1]
		c_C2=pedidoCombo2[contPedidos-1]
		c_C3=pedidoCombo3[contPedidos-1]
		
		pBolsa=pedidoBolsa[contPedidos-1]
		pBalde=pedidoBalde[contPedidos-1]
		pLata=pedidoLata[contPedidos-1]
		
		coca=pedidoCoca[contPedidos-1]
		sprite=pedidoSprite[contPedidos-1]
		agua=pedidoAgua[contPedidos-1]
		
		mym=pedidoMym[contPedidos-1]
		mogul=pedidoMogul[contPedidos-1]
		beldent=pedidoBeldent[contPedidos-1]
		
		contPedidos=contPedidos-1
		contEntr=contEntr-cantEntradas
		
	SiNo
		Para j=1 Hasta pedidoEntradas[contPedidos] Con Paso 1 Hacer
			matriz[entradaFila[j],entradaColumna[j]]="[ ]"
		Fin Para
		contPedidos=contPedidos-1
		contEntr=contEntr-cantEntradas
		c_C1=0
		c_C2=0
		c_C3=0
		pBolsa=0
		pBalde=0
		pLata=0
		coca=0
