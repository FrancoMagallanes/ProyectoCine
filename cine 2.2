funcion cantidadPelis(entradaPel,contEntr,pel,numEleccion,cantTotal Por Referencia)
	
	
	Para i=1 Hasta contEntr Con Paso 1 Hacer
		si entradaPel[i]=pel[numEleccion] Entonces
			cantTotal=cantTotal+1
		FinSi
	FinPara
	
	Escribir "Cantidad entradas ",pel[numEleccion],": ",cantTotal
	cantTotal=0
	
FinFuncion


Funcion RecaudacionPelis(recTotal Por Referencia,contEntr,pel,numEleccion,entradaPel,precioEntradas)
	
	Para i=1 Hasta contEntr Con Paso 1 Hacer
		si entradaPel[i]=pel[numEleccion] Entonces
			recTotal=recTotal+precioEntradas
		FinSi
	FinPara
	
	Escribir "Recaudacion ",pel[numEleccion],": $",recTotal
	recTotal=0
	
FinFuncion


